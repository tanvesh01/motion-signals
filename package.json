{
  "name": "motion-signals",
  "version": "0.0.2",
  "description": "A wrapper over Motion One, An animation library, built on the Web Animations API for the smallest filesize and the fastest performance.",
  "author": "tanvesh01",
  "license": "MIT",
  "repository": "tanvesh01/motion-signals",
  "homepage": "https://github.com/tanvesh01/motion-signals",
  "bugs": {
    "url": "https://github.com/tanvesh01/motion-signals/issues",
    "email": "sarvetanvesh01@gmail.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "peerDependencies": {
    "motion": "~10.6.1",
    "solid-js": "~1.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "jest": "^26.1.0",
    "rollup": "^2.57.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "npm"
  ]
}
